#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/harrylow/TappGlobalWebApp/harrylow/TappGlobalWebApp/node_modules/.pnpm/superstatic@9.0.3/node_modules/superstatic/lib/bin/node_modules:/Users/harrylow/TappGlobalWebApp/harrylow/TappGlobalWebApp/node_modules/.pnpm/superstatic@9.0.3/node_modules/superstatic/lib/node_modules:/Users/harrylow/TappGlobalWebApp/harrylow/TappGlobalWebApp/node_modules/.pnpm/superstatic@9.0.3/node_modules/superstatic/node_modules:/Users/harrylow/TappGlobalWebApp/harrylow/TappGlobalWebApp/node_modules/.pnpm/superstatic@9.0.3/node_modules:/Users/harrylow/TappGlobalWebApp/harrylow/TappGlobalWebApp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/harrylow/TappGlobalWebApp/harrylow/TappGlobalWebApp/node_modules/.pnpm/superstatic@9.0.3/node_modules/superstatic/lib/bin/node_modules:/Users/harrylow/TappGlobalWebApp/harrylow/TappGlobalWebApp/node_modules/.pnpm/superstatic@9.0.3/node_modules/superstatic/lib/node_modules:/Users/harrylow/TappGlobalWebApp/harrylow/TappGlobalWebApp/node_modules/.pnpm/superstatic@9.0.3/node_modules/superstatic/node_modules:/Users/harrylow/TappGlobalWebApp/harrylow/TappGlobalWebApp/node_modules/.pnpm/superstatic@9.0.3/node_modules:/Users/harrylow/TappGlobalWebApp/harrylow/TappGlobalWebApp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../superstatic/lib/bin/server.js" "$@"
else
  exec node  "$basedir/../superstatic/lib/bin/server.js" "$@"
fi
